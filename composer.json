            }
        }
    },
    "authors": [
        {
            "name": "manu-applab",
            "email": "manu@applab.qa"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
